<div class="row">
   <div id="months" class="col-md-1">
		<div class="row">
			<h5>Months</h5>
		</div>
		<div class="row">
			<a href="/archive/july"><h5>July</h5></a>
		</div>
		<div class="row">
			<a href="/archive/august"><h5>Aug</h5></a>
		</div>
		<div class="row">
			<a href="/archive/september"><h5>Sep</h5></a>
		</div>
		<div class="row">
			<a href="/archive/october"><h5>Oct</h5></a>
		</div>
	</div>
    <div class="col-md-5">
    	<% @posts.each do |post| %>
   			<article><a href="/post/<%= post.id %>">
   			<%= post.title %>
   			<p><%= post.content %></p>
   			</a></article>
   		<% end %>
    </div>
  </div>
</div>

  